<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ampi__IATI_Tied_Status_Code__c</fullName>
    <deprecated>false</deprecated>
    <description>This formula field returns code for IATI reporting based on project status mentioned in IATI Tied Status field</description>
    <externalId>false</externalId>
    <formula>CASE(ampi__IATI_Tied_Status__c,&quot;Partially tied&quot;,3,&quot;Tied&quot;,4,&quot;Untied&quot;,5,null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Code corresponding to the IATI Tied Status</inlineHelpText>
    <label>IATI Tied Status code</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
