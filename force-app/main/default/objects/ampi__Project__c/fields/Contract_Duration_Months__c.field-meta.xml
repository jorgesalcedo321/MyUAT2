<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contract_Duration_Months__c</fullName>
    <externalId>false</externalId>
    <formula>((Contract_Eligibility_End_Date__c - Contract_Eligibility_Start_Date__c) / 365 )* 12</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The duration is calculated automatically by the system based on contract eligibility start and end date.</inlineHelpText>
    <label>Contract Duration (Months)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
