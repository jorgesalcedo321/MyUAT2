<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ampi__IATI_Project_Status_Code__c</fullName>
    <deprecated>false</deprecated>
    <description>This formula field returns code for IATI reporting based on project status mentioned in IATI Project Status field</description>
    <externalId>false</externalId>
    <formula>CASE (ampi__IATI_Project_Status__c, &quot;Pipeline/identification&quot;,1,
&quot;Implementation&quot;,2,
&quot;Completion&quot;,3,
&quot;Post-completion&quot;,4,
&quot;Cancelled&quot;,5,
&quot;Suspended&quot;,6,null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>IATI Project Status Code</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
